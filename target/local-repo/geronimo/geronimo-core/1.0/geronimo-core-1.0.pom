  <build>
    <sourceDirectory>/home/hogstrom/geronimo-1.0-src/modules/core/src/java</sourceDirectory>
    <testSourceDirectory>/home/hogstrom/geronimo-1.0-src/modules/core/src/test</testSourceDirectory>
    <resources>
      <resource>
        <directory>/home/hogstrom/geronimo-1.0-src/modules/core/src/java</directory>
        <includes>
          <include>**/*.properties</include>
        </includes>
      </resource>
      <resource>
        <directory>/home/hogstrom/geronimo-1.0-src/modules/core/src/resources</directory>
        <includes>
          <include>**/*</include>
        </includes>
      </resource>
      <resource>
        <directory>/home/hogstrom/geronimo-1.0-src/modules/core/target/etc</directory>
        <includes>
          <include>META-INF/geronimo-service.xml</include>
        </includes>
      </resource>
    </resources>
    <testResources>
      <testResource>
        <directory>/home/hogstrom/geronimo-1.0-src/modules/core/src/test-data</directory>
        <includes>
          <include>*.properties</include>
        </includes>
      </testResource>
      <testResource>
        <directory>/home/hogstrom/geronimo-1.0-src/modules/core/src/schema</directory>
        <includes>
          <include>*.xsd</include>
          <include>*.dtd</include>
        </includes>
      </testResource>
    </testResources>
    <plugins>
      <plugin>
        <artifactId>surefire</artifactId>
        <configuration>
          <includes>
            <include>**/*Test.java</include>
          </includes>
          <excludes>
            <exclude>**/Abstract*.java</exclude>
          </excludes>
        </configuration>
      </plugin>
    </plugins>
  </build>
